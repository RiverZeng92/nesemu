{
  "name": "nesemu",
  "version": "0.0.1",
  "description": "NES Emulator",
  "main": "index.js",
  "scripts": {
    "start": "npm run default",
    "default": "gulp",
    "build": "gulp build",
    "watch": "gulp watch",
    "server": "webpack-dev-server --content-base public",
    "test": "gulp test",
    "lint": "gulp lint",
    "release": "rm -rf nodejs && gulp release && git checkout nodejs",
    "clean": "gulp clean",
    "deploy": "pushd gh-pages && git reset --hard && git checkout gh-pages && popd && npm run release && cp -r release/* gh-pages/ && pushd gh-pages && git commit -a --amend -m 'Deploy' && git push -f && popd"
  },
  "author": "tyfkda",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyfkda/nesemu"
  },
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/core": "~7.5.0",
    "@babel/preset-env": "~7.5.0",
    "@babel/register": "~7.4.4",
    "@types/jest": "~24.0.15",
    "@types/node": "~12.0.12",
    "argv": "~0.0.2",
    "babel-jest": "~24.8.0",
    "babel-loader": "~8.0.6",
    "browser-sync": "~2.26.7",
    "clone": "~2.1.2",
    "del": "~5.0.0",
    "gulp": "~4.0.2",
    "gulp-cssnano": "~2.1.3",
    "gulp-ejs": "~4.1.1",
    "gulp-htmlmin": "~5.0.1",
    "gulp-jest": "~4.0.2",
    "gulp-plumber": "~1.2.1",
    "gulp-sass": "~4.0.2",
    "gulp-tslint": "~8.1.4",
    "gulp-util": "~3.0.7",
    "jest": "~24.8.0",
    "jest-cli": "~24.8.0",
    "ts-jest": "~24.0.2",
    "ts-loader": "~6.0.4",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.5.2",
    "webpack": "~4.35.2",
    "webpack-stream": "~5.2.1"
  },
  "dependencies": {
    "core-js": "~3.1.4",
    "jszip": "~3.2.2",
    "md5": "git+https://github.com/pvorb/node-md5.git#master",
    "stats-js": "~1.0.1"
  }
}
